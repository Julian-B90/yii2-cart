{
    "name": "hscstudio/yii2-cart",
    "description": "Yii2 extension that adds shopping cart functions",
    "keywords": [
        "yii",
        "shopping cart"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Hafid Mukhlasin",
            "email": "hafidmukhlasin@gmail.com"
        }
    ],
    "type": "yii2-extension",
    "require": {
        "php": ">=5.6.0",
        "yiisoft/yii2": "~2.0.14"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "hscstudio\\cart\\": ""
        }
    }
}